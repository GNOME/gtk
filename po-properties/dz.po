msgid "FALSE removes outside bevel from entry"
msgstr "རྫུན་མ་གིས་ ཕྱི་ཁའི་སེར་ཁ་ ཐོ་བཀོད་ནང་ལས་ རྩ་བསྐྲད་གཏངམ་ཨིན།"

#: gtk/gtkentry.c:543
msgid "Invisible character"
msgstr "མ་མཐོང་པའི་ཡིག་འབྲུ།"

