#: ../gtk/deprecated/gtkhandlebox.c:222 ../gtk/gtkcombobox.c:1071
#: ../gtk/gtkentry.c:941 ../gtk/gtkmenubar.c:216 ../gtk/gtkstatusbar.c:165
#: ../gtk/gtktoolbar.c:632 ../gtk/gtkviewport.c:178
