<interface>
  <template class="PaintableEditor" parent="GtkWidget">
    <child>
      <object class="GtkScrolledWindow" id="swin">
        <property name="hscrollbar-policy">never</property>
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkGrid">
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="yes">Keywords</property>
                    <property name="xalign">1</property>
                    <layout>
                      <property name="row">0</property>
                      <property name="column">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkEntry" id="keywords">
                    <signal name="activate" handler="keywords_changed" swapped="yes"/>
                    <layout>
                      <property name="row">0</property>
                      <property name="column">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="yes">Size</property>
                    <property name="xalign">1</property>
                    <layout>
                      <property name="row">4</property>
                      <property name="column">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkBox">
                    <child>
                      <object class="GtkEntry" id="width">
                        <signal name="activate" handler="size_changed" swapped="yes"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel">
                        <property name="label">âœ•</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkEntry" id="height">
                        <signal name="activate" handler="size_changed" swapped="yes"/>
                      </object>
                    </child>
                    <layout>
                      <property name="row">4</property>
                      <property name="column">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="yes">Compatibility</property>
                    <property name="xalign">1</property>
                    <layout>
                      <property name="row">6</property>
                      <property name="column">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="compat">
                    <property name="xalign">0</property>
                    <layout>
                      <property name="row">6</property>
                      <property name="column">1</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkLabel" id="summary1">
                    <property name="xalign">0</property>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="summary2">
                    <property name="xalign">0</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="path_elts">
                <property name="orientation">vertical</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="add_path">
                <property name="label" translatable="yes">Add path</property>
                <property name="action-name">win.add-path</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
