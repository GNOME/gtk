<interface>
  <template class="PaintableEditor" parent="GtkWidget">
    <child>
      <object class="GtkScrolledWindow" id="swin">
        <property name="hscrollbar-policy">never</property>
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkGrid">
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="yes">Duration</property>
                    <property name="xalign">1</property>
                    <layout>
                      <property name="row">1</property>
                      <property name="column">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkSpinButton">
                    <property name="adjustment">
                      <object class="GtkAdjustment" id="duration_adj">
                        <property name="lower">0</property>
                        <property name="upper">3</property>
                        <property name="value">1.5</property>
                        <property name="step-increment">0.1</property>
                        <property name="page-increment">0.1</property>
                      </object>
                    </property>
                    <property name="numeric">1</property>
                    <property name="digits">2</property>
                    <layout>
                      <property name="row">1</property>
                      <property name="column">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="yes">Delay</property>
                    <property name="xalign">1</property>
                    <layout>
                      <property name="row">2</property>
                      <property name="column">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkSpinButton">
                    <property name="adjustment">
                      <object class="GtkAdjustment" id="delay_adj">
                        <property name="lower">0</property>
                        <property name="upper">3</property>
                        <property name="value">1.5</property>
                        <property name="step-increment">0.1</property>
                        <property name="page-increment">0.1</property>
                      </object>
                    </property>
                    <property name="numeric">1</property>
                    <property name="digits">2</property>
                    <layout>
                      <property name="row">2</property>
                      <property name="column">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="yes">Easing</property>
                    <property name="xalign">1</property>
                    <layout>
                      <property name="row">3</property>
                      <property name="column">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkDropDown" id="easing">
                    <property name="model">
                      <object class="GtkStringList">
                        <items>
                          <item>Linear</item>
                          <item>Ease-in-out</item>
                          <item>Ease-in</item>
                          <item>Ease-out</item>
                          <item>Ease</item>
                        </items>
                      </object>
                    </property>
                    <layout>
                      <property name="row">3</property>
                      <property name="column">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="yes">Size</property>
                    <property name="xalign">1</property>
                    <layout>
                      <property name="row">4</property>
                      <property name="column">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkBox">
                    <child>
                      <object class="GtkEntry" id="width">
                        <signal name="activate" handler="size_changed" swapped="yes"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel">
                        <property name="label">âœ•</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkEntry" id="height">
                        <signal name="activate" handler="size_changed" swapped="yes"/>
                      </object>
                    </child>
                    <layout>
                      <property name="row">4</property>
                      <property name="column">1</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="path_elts">
                <property name="orientation">vertical</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="add_path">
                <property name="tooltip-text" translatable="yes">Add a path</property>
                <property name="action-name">win.add-path</property>
                <property name="child">
                  <object class="GtkImage">
                    <property name="paintable">
                      <object class="GtkIconPaintable">
                        <property name="file">resource:///org/gtk/libgtk/icons/scalable/actions/list-add-symbolic.svg</property>
                      </object>
                    </property>
                  </object>
                </property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
