<interface>
  <template class="PaintableEditor" parent="GtkWidget">
    <child>
      <object class="GtkScrolledWindow" id="swin">
        <property name="hscrollbar-policy">never</property>
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkGrid">
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="yes">_Keywords</property>
                    <property name="use-underline">1</property>
                    <property name="mnemonic-widget">keywords</property>
                    <property name="xalign">1</property>
                    <layout>
                      <property name="row">0</property>
                      <property name="column">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkEntry" id="keywords">
                    <signal name="activate" handler="keywords_changed" swapped="yes"/>
                    <layout>
                      <property name="row">0</property>
                      <property name="column">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="yes">_Size</property>
                    <property name="use-underline">1</property>
                    <property name="mnemonic-widget">width</property>
                    <property name="xalign">1</property>
                        <accessibility>
                          <property name="label" translatable="yes">Width</property>
                        </accessibility>
                    <layout>
                      <property name="row">4</property>
                      <property name="column">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkBox">
                    <child>
                      <object class="GtkEntry" id="width">
                        <signal name="activate" handler="size_changed" swapped="yes"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel">
                        <property name="label">âœ•</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkEntry" id="height">
                        <signal name="activate" handler="size_changed" swapped="yes"/>
                        <accessibility>
                          <property name="label" translatable="yes">Height</property>
                        </accessibility>
                      </object>
                    </child>
                    <layout>
                      <property name="row">4</property>
                      <property name="column">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="yes">_Initial state</property>
                    <property name="use-underline">1</property>
                    <property name="mnemonic-widget">initial_state</property>
                    <property name="xalign">1</property>
                    <layout>
                      <property name="row">5</property>
                      <property name="column">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkSpinButton" id="initial_state">
                    <property name="numeric">1</property>
                    <property name="digits">0</property>
                    <property name="adjustment">
                      <object class="GtkAdjustment">
                        <property name="lower">-1</property>
                        <property name="upper">63</property>
                        <property name="step-increment">1</property>
                        <property name="page-increment">1</property>
                        <property name="value" bind-source="PaintableEditor" bind-property="initial-state" bind-flags="bidirectional|sync-create"/>
                      </object>
                    </property>
                    <layout>
                      <property name="row">5</property>
                      <property name="column">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="yes">_Compatibility</property>
                    <property name="use-underline">1</property>
                    <property name="mnemonic-widget">compat</property>
                    <property name="xalign">1</property>
                    <layout>
                      <property name="row">6</property>
                      <property name="column">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="compat">
                    <property name="xalign">0</property>
                    <property name="selectable">1</property>
                    <layout>
                      <property name="row">6</property>
                      <property name="column">1</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkLabel" id="summary1">
                    <property name="selectable">1</property>
                    <property name="xalign">0</property>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="summary2">
                    <property name="selectable">1</property>
                    <property name="xalign">0</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="path_elts">
                <property name="orientation">vertical</property>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <property name="hexpand">0</property>
                <child>
                  <object class="GtkButton" id="add_path">
                    <property name="label" translatable="yes">Add path</property>
                    <property name="hexpand">1</property>
                    <property name="action-name">win.add-path</property>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="edit_states">
                    <property name="label" translatable="yes">States</property>
                    <property name="action-name">win.edit-states</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
