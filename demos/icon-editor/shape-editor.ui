              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="line_start_label">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool_and_uint_equal">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">shape_expander</lookup>
                <lookup name="selected">shape_dropdown</lookup>
                <constant type='guint'>0</constant>
