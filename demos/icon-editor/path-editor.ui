<interface>
  <template class="PathEditor" parent="GtkWidget">
    <child>
      <object class="GtkGrid" id="grid">
        <child>
          <object class="GtkExpander" id="expander">
            <property name="hexpand">0</property>
            <property name="valign">center</property>
            <property name="label-widget">
              <object class="GtkBox">
                <property name="hexpand">0</property>
                <child>
                  <object class="GtkImage">
                    <property name="pixel-size">20</property>
                    <property name="paintable" bind-source="PathEditor" bind-property="path-image"/>
                    <accessibility>
                      <property name="label" translatable="yes">Path</property>
                    </accessibility>
                  </object>
                </child>
                <child>
                  <object class="GtkEditableLabel" id="id_label">
                    <property name="hexpand">1</property>
                    <property name="xalign">0</property>
                    <signal name="notify::text" handler="id_changed" swapped="yes"/>
                  </object>
                </child>
                <child>
                  <object class="GtkBox">
                    <style><class name="tight"/></style>
                    <child>
                      <object class="GtkButton">
                        <style>
                          <class name="circular"/>
                          <class name="small"/>
                        </style>
                        <property name="focus-on-click">0</property>
                        <property name="has-frame">0</property>
                        <property name="tooltip-text" translatable="yes">Edit path geometry</property>
                        <property name="child">
                          <object class="GtkImage">
                            <property name="paintable">
                              <object class="GtkIconPaintable">
                                <property name="file">resource:///org/gtk/Shaper/document-edit-symbolic.svg</property>
                              </object>
                            </property>
                          </object>
                        </property>
                        <signal name="clicked" handler="edit_path" swapped="yes"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkButton">
                        <style>
                          <class name="circular"/>
                          <class name="small"/>
                        </style>
                        <property name="focus-on-click">0</property>
                        <property name="has-frame">0</property>
                        <property name="tooltip-text" translatable="yes">Duplicate path</property>
                        <property name="child">
                          <object class="GtkImage">
                            <property name="paintable">
                              <object class="GtkIconPaintable">
                                <property name="file">resource:///org/gtk/Shaper/plus-symbolic.svg</property>
                              </object>
                            </property>
                          </object>
                        </property>
                        <signal name="clicked" handler="duplicate_path" swapped="yes"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkButton" id="move_down">
                        <style>
                          <class name="circular"/>
                          <class name="small"/>
                        </style>
                        <property name="focus-on-click">0</property>
                        <property name="has-frame">0</property>
                        <property name="tooltip-text" translatable="yes">Move path down</property>
                        <property name="child">
                          <object class="GtkImage">
                            <property name="paintable">
                              <object class="GtkIconPaintable">
                                <property name="file">resource:///org/gtk/libgtk/icons/go-down-symbolic.svg</property>
                              </object>
                            </property>
                          </object>
                        </property>
                        <signal name="clicked" handler="move_path_down" swapped="yes"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkButton">
                        <style>
                          <class name="circular"/>
                          <class name="small"/>
                        </style>
                        <property name="focus-on-click">0</property>
                        <property name="has-frame">0</property>
                        <property name="tooltip-text" translatable="yes">Delete path</property>
                        <property name="child">
                          <object class="GtkImage">
                            <property name="paintable">
                              <object class="GtkIconPaintable">
                                <property name="file">resource:///org/gtk/libgtk/icons/user-trash-symbolic.svg</property>
                              </object>
                            </property>
                          </object>
                        </property>
                        <signal name="clicked" handler="delete_path" swapped="yes"/>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </property>
            <layout>
              <property name="row">0</property>
              <property name="column">0</property>
              <property name="column-span">2</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="path_label">
            <property name="visible" bind-source="expander" bind-property="expanded" bind-flags="sync-create"/>
            <property name="label" translatable="yes">_Path</property>
            <property name="use-underline">1</property>
            <property name="mnemonic-widget">path_cmds</property>
            <property name="xalign">1</property>
            <layout>
              <property name="row">1</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="path_cmds">
            <property name="visible" bind-source="expander" bind-property="expanded" bind-flags="sync-create"/>
            <property name="ellipsize">end</property>
            <layout>
              <property name="row">1</property>
              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="origin_label">
            <property name="visible" bind-source="expander" bind-property="expanded" bind-flags="sync-create"/>
            <property name="label" translatable="yes">_Origin</property>
            <property name="use-underline">1</property>
            <property name="mnemonic-widget">origin</property>
            <property name="xalign">1</property>
            <layout>
              <property name="row">2</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkScale" id="origin">
            <property name="visible" bind-source="expander" bind-property="expanded" bind-flags="sync-create"/>
            <signal name="value-changed" handler="origin_changed" swapped="yes"/>
            <property name="adjustment">
              <object class="GtkAdjustment">
                <property name="lower">0</property>
                <property name="upper">1</property>
                <property name="step-increment">0.1</property>
              </object>
            </property>
            <marks>
              <mark value="0" position="bottom"/>
              <mark value="0.5" position="bottom"/>
              <mark value="1" position="bottom"/>
            </marks>
            <layout>
              <property name="row">2</property>
              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkExpander" id="transition_expander">
            <property name="visible" bind-source="expander" bind-property="expanded" bind-flags="sync-create"/>
            <property name="hexpand">0</property>
            <property name="valign">center</property>
            <property name="margin-start">10</property>
            <property name="label-widget">
              <object class="GtkLabel" id="transition_label">
                <property name="hexpand">1</property>
                <property name="xalign">1</property>
                <property name="label" translatable="yes">_Transition</property>
                <property name="use-underline">1</property>
                <property name="mnemonic-widget">transition_type</property>
              </object>
            </property>
            <layout>
              <property name="row">5</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkDropDown" id="transition_type">
            <property name="visible" bind-source="expander" bind-property="expanded" bind-flags="sync-create"/>
            <signal name="notify::selected" handler="transition_changed" swapped="yes"/>
            <property name="model">
              <object class="GtkStringList">
                <items>
                  <item>None</item>
                  <item>Animate</item>
                  <item>Morph</item>
                  <item>Fade</item>
                </items>
              </object>
            </property>
            <layout>
              <property name="row">5</property>
              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="transition_duration_label">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">transition_expander</lookup>
              </closure>
            </binding>
            <property name="label" translatable="yes">_Duration</property>
            <property name="use-underline">1</property>
            <property name="mnemonic-widget">transition_duration</property>
            <property name="xalign">1</property>
            <layout>
              <property name="row">6</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkSpinButton" id="transition_duration">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">transition_expander</lookup>
              </closure>
            </binding>
            <signal name="value-changed" handler="transition_changed" swapped="yes"/>
            <property name="adjustment">
              <object class="GtkAdjustment">
                <property name="lower">0</property>
                <property name="upper">3000</property>
                <property name="step-increment">1</property>
              </object>
            </property>
            <property name="numeric">1</property>
            <property name="digits">2</property>
            <layout>
              <property name="row">6</property>
              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="transition_delay_label">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">transition_expander</lookup>
              </closure>
            </binding>
            <property name="label" translatable="yes">D_elay</property>
            <property name="use-underline">1</property>
            <property name="mnemonic-widget">transition_delay</property>
            <property name="xalign">1</property>
            <layout>
              <property name="row">7</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkSpinButton" id="transition_delay">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">transition_expander</lookup>
              </closure>
            </binding>
            <signal name="value-changed" handler="transition_changed" swapped="yes"/>
            <property name="adjustment">
              <object class="GtkAdjustment">
                <property name="lower">-3000</property>
                <property name="upper">3000</property>
                <property name="step-increment">1</property>
              </object>
            </property>
            <property name="numeric">1</property>
            <property name="digits">2</property>
            <layout>
              <property name="row">7</property>
              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="transition_easing_label">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">transition_expander</lookup>
              </closure>
            </binding>
            <property name="label" translatable="yes">_Easing</property>
            <property name="use-underline">1</property>
            <property name="mnemonic-widget">transition_easing</property>
            <property name="xalign">1</property>
            <layout>
              <property name="row">8</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">transition_expander</lookup>
              </closure>
            </binding>
            <property name="hexpand">0</property>
            <child>
              <object class="GtkDropDown" id="transition_easing">
                <property name="valign">center</property>
                <property name="hexpand">1</property>
                <signal name="notify::selected" handler="transition_changed" swapped="yes"/>
                <property name="model">
                  <object class="GtkStringList">
                    <items>
                      <item>Linear</item>
                      <item>Ease-in-out</item>
                      <item>Ease-in</item>
                      <item>Ease-out</item>
                      <item>Ease</item>
                    </items>
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkMenuButton">
                <property name="sensitive">0</property>
                <property name="valign">center</property>
                <property name="child">
                  <object class="MiniGraph">
                    <property name="width-request">50</property>
                    <property name="height-request">50</property>
                    <property name="easing-function" bind-source="transition_easing" bind-property="selected" bind-flags="sync-create"/>
                  </object>
                </property>
              </object>
            </child>
            <layout>
              <property name="row">8</property>
              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkExpander" id="animation_expander">
            <property name="visible" bind-source="expander" bind-property="expanded" bind-flags="sync-create"/>
            <property name="hexpand">0</property>
            <property name="valign">center</property>
            <property name="margin-start">10</property>
            <property name="label-widget">
              <object class="GtkLabel" id="animation_label">
                <property name="hexpand">1</property>
                <property name="xalign">1</property>
                <property name="label" translatable="yes">_Animation</property>
                <property name="use-underline">1</property>
                <property name="mnemonic-widget">animation_type</property>
              </object>
            </property>
            <layout>
              <property name="row">9</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkDropDown" id="animation_type">
            <property name="visible" bind-source="expander" bind-property="expanded" bind-flags="sync-create"/>
            <signal name="notify::selected" handler="animation_changed" swapped="yes"/>
            <property name="model">
              <object class="GtkStringList">
                <items>
                  <item>None</item>
                  <item>Automatic</item>
                </items>
              </object>
            </property>
            <layout>
              <property name="row">9</property>
              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="animation_direction_label">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">animation_expander</lookup>
              </closure>
            </binding>
            <property name="label" translatable="yes">_Direction</property>
            <property name="use-underline">1</property>
            <property name="mnemonic-widget">animation_direction</property>
            <property name="xalign">1</property>
            <layout>
              <property name="row">10</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkDropDown" id="animation_direction">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">animation_expander</lookup>
              </closure>
            </binding>
            <signal name="notify::selected" handler="animation_changed" swapped="yes"/>
            <property name="model">
              <object class="GtkStringList">
                <items>
                  <item>Normal</item>
                  <item>Alternate</item>
                  <item>Reverse</item>
                  <item>Reverse Alternate</item>
                  <item>In-Out</item>
                  <item>In-Out Alternate</item>
                  <item>In-Out Reverse</item>
                  <item>Segment</item>
                  <item>Segment Alternate</item>
                </items>
              </object>
            </property>
            <layout>
              <property name="row">10</property>
              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="animation_duration_label">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">animation_expander</lookup>
              </closure>
            </binding>
            <property name="label" translatable="yes">_Duration</property>
            <property name="use-underline">1</property>
            <property name="mnemonic-widget">animation_duration</property>
            <property name="xalign">1</property>
            <layout>
              <property name="row">11</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkSpinButton" id="animation_duration">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">animation_expander</lookup>
              </closure>
            </binding>
            <signal name="value-changed" handler="animation_changed" swapped="yes"/>
            <property name="adjustment">
              <object class="GtkAdjustment">
                <property name="lower">0</property>
                <property name="upper">3000</property>
                <property name="step-increment">1</property>
              </object>
            </property>
            <property name="numeric">1</property>
            <property name="digits">2</property>
            <layout>
              <property name="row">11</property>
              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="animation_repeat_label">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">animation_expander</lookup>
              </closure>
            </binding>
            <property name="label" translatable="yes">_Repetitions</property>
            <property name="use-underline">1</property>
            <property name="mnemonic-widget">animation_repeat</property>
            <property name="xalign">1</property>
            <layout>
              <property name="row">12</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">animation_expander</lookup>
              </closure>
            </binding>
            <child>
              <object class="GtkCheckButton" id="infty_check">
                <property name="label">∞</property>
                <accessibility>
                  <property name="label" translatable="yes">Repeat forever</property>
                </accessibility>
                <signal name="notify::active" handler="animation_changed" swapped="yes"/>
              </object>
            </child>
            <child>
              <object class="GtkSpinButton" id="animation_repeat">
                <property name="sensitive" bind-source="infty_check" bind-property="active" bind-flags="sync-create | invert-boolean"/>
                <signal name="value-changed" handler="animation_changed" swapped="yes"/>
                <property name="adjustment">
                  <object class="GtkAdjustment">
                    <property name="lower">0</property>
                    <property name="upper">3.402823e+38</property>
                    <property name="step-increment">0.5</property>
                  </object>
                </property>
                <property name="digits">2</property>
              </object>
            </child>
            <layout>
              <property name="row">12</property>
              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="animation_easing_label">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">animation_expander</lookup>
              </closure>
            </binding>
            <property name="label" translatable="yes">_Easing</property>
            <property name="use-underline">1</property>
            <property name="mnemonic-widget">animation_easing</property>
            <property name="xalign">1</property>
            <layout>
              <property name="row">13</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">animation_expander</lookup>
              </closure>
            </binding>
            <property name="hexpand">0</property>
            <child>
              <object class="GtkDropDown" id="animation_easing">
                <property name="valign">center</property>
                <property name="hexpand">1</property>
                <signal name="notify::selected" handler="animation_changed" swapped="yes"/>
                <property name="model">
                  <object class="GtkStringList">
                    <items>
                      <item>Linear</item>
                      <item>Ease-in-out</item>
                      <item>Ease-in</item>
                      <item>Ease-out</item>
                      <item>Ease</item>
                    </items>
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkMenuButton">
                <property name="valign">center</property>
                <property name="child">
                  <object class="MiniGraph" id="mini_graph">
                    <property name="width-request">50</property>
                    <property name="height-request">50</property>
                    <property name="easing-function" bind-source="animation_easing" bind-property="selected" bind-flags="sync-create"/>
                  </object>
                </property>
              </object>
            </child>
            <layout>
              <property name="row">13</property>
              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="animation_segment_label">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">animation_expander</lookup>
              </closure>
            </binding>
            <property name="label" translatable="yes">Segment</property>
            <property name="use-underline">1</property>
            <property name="mnemonic-widget">animation_segment</property>
            <property name="xalign">1</property>
            <layout>
              <property name="row">15</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkSpinButton" id="animation_segment">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">animation_expander</lookup>
              </closure>
            </binding>
            <signal name="value-changed" handler="animation_changed" swapped="yes"/>
            <property name="adjustment">
              <object class="GtkAdjustment">
                <property name="lower">0</property>
                <property name="upper">1</property>
                <property name="step-increment">0.1</property>
              </object>
            </property>
            <property name="numeric">1</property>
            <property name="digits">2</property>
            <layout>
              <property name="row">15</property>
              <property name="column">1</property>
            </layout>
          </object>
        </child>

        <child>
          <object class="GtkExpander" id="stroke_expander">
            <property name="visible" bind-source="expander" bind-property="expanded" bind-flags="sync-create"/>
            <property name="hexpand">0</property>
            <property name="valign">center</property>
            <property name="margin-start">10</property>
            <property name="label-widget">
              <object class="GtkLabel">
                <property name="hexpand">1</property>
                <property name="xalign">1</property>
                <property name="label" translatable="yes">_Stroke</property>
                <property name="use-underline">1</property>
                <property name="mnemonic-widget">stroke_paint</property>
              </object>
            </property>
            <layout>
              <property name="row">16</property>
              <property name="column">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ColorEditor" id="stroke_paint">
            <property name="visible" bind-source="expander" bind-property="expanded" bind-flags="sync-create"/>
            <property name="hexpand">0</property>
            <layout>
              <property name="row">16</property>
              <property name="column">1</property>
            </layout>
            <signal name="notify::symbolic" handler="stroke_changed" swapped="yes"/>
            <signal name="notify::color" handler="stroke_changed" swapped="yes"/>
          </object>
        </child>

        <child>
          <object class="GtkExpander" id="width_expander">
            <binding name="visible">
              <closure type="gboolean" function="bool_and_bool">
                <lookup name="expanded">expander</lookup>
                <lookup name="expanded">stroke_expander</lookup>
              </closure>
