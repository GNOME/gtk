            </marks>
          </object>
        </child>
        <child>
          <object class="GtkScale">
            <property name="adjustment">adjustment1</property>
            <property name="draw-value">0</property>
            <marks>
              <mark value="10" position="top"/>
              <mark value="20" position="bottom"/>
            </marks>
          </object>
        </child>
        <child>
          <object class="GtkScale">
            <property name="adjustment">adjustment1</property>
            <property name="draw-value">0</property>
            <marks>
              <mark value="10" position="bottom"/>
              <mark value="20" position="bottom"/>
            </marks>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
