clip {
  clip: 0 0 300 200;
  child: container {
    color {
      bounds: 25 25 50 50;
      color: rgb(255,0,0);
    }
    color {
      bounds: 100 0 100 100;
      color: rgb(0,0,255);
    }
    rounded-clip {
      clip: 220 20 60 60 / 30;
      child: color {
        bounds: 220 20 60 60;
        color: rgb(0,128,0);
      }
    }
    color {
      bounds: 50 100 200 200;
      color: rgb(255,255,0);
    }
  }
}
