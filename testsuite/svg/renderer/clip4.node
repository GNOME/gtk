mask {
  source: color {
    bounds: 0 25 50 50;
    color: rgb(255,0,0);
  }
  mask: rounded-clip {
    clip: 10 10 80 80 / 40;
    child: transform {
      transform: translate(0, 25) scale(0.5);
      child: fill {
        child: color {
          bounds: 0 0 200 100;
          color: rgb(0,0,0);
        }
        path: "\
M 0 0 h 200 v 100 h -200 z";
        fill-rule: winding;
      }
    }
  }
}
