isolation {
  child: container {
    fill {
      child: color {
        bounds: 10 10 80 80;
        color: rgb(0,0,0);
      }
      path: "\
M 10 10\
L 90 90";
      fill-rule: winding;
    }
    stroke {
      child: color {
        bounds: 6 6 88 88;
        color: rgb(255,0,0);
      }
      path: "\
M 10 10\
L 90 90";
      line-width: 1;
      line-cap: butt;
      line-join: miter;
    }
  }
  isolations: background;
}
