@cicp "cicp1" {
  primaries: 6;
  transfer: 6;
  matrix: 0;
  range: full;
}

linear-gradient {
  bounds: 10 10 100 100;
  start: 2 3;
  end: 20 40;
  stops: 0 rgb(0, 0, 0), 0.5 color(rec2100-pq 0.4 0.5 0.6), 1 #AF0;
  interpolation: srgb;
  hue-interpolation: longer;
}

linear-gradient {
  bounds: 10 10 100 100;
  start: 2 3;
  end: 20 40;
  stops: 0 rgb(0, 0, 0), 0.5 0.3 color(rec2100-pq 0.4 0.5 0.6), 1 #AF0;
  interpolation: srgb;
  hue-interpolation: longer;
}

radial-gradient {
  bounds: 10 10 100 100;
  center: 25 26;
  hradius: 10;
  vradius: 20;
  start: 0.1;
  end: 0.9;
  stops: 0 rgb(0, 0, 0), 0.5 color(rec2100-pq 0.4 0.5 0.6), 1 #AF0;
  interpolation: rec2100-pq;
  hue-interpolation: increasing;
}

conic-gradient {
  bounds: 10 10 100 100;
  center: 25 26;
  rotation: 200;
  stops: 0 rgb(0, 0, 0), 0.5 color(rec2100-pq 0.4 0.5 0.6), 1 #AF0;
  interpolation: "cicp1";
}
